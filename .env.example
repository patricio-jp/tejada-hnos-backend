# Example environment variables for tejada-hnos-backend
# Rename this file to .env and change the values for your environment

# Node environment
NODE_ENV=development

# Application port
PORT=3000

# JWT signing secret
JWT_SECRET=your_jwt_secret_here

# Database connection URL (MongoDB)
MONGODB_URI=mongodb://localhost:27017/tejada_hnos

# Database (MySQL)
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USERNAME=root
MYSQL_PASSWORD=root
MYSQL_DATABASE=tejada_hnos
