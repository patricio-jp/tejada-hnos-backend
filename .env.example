# Example environment variables for tejada-hnos-backend
# Rename this file to .env and change the values for your environment

# Node environment
NODE_ENV=development

# Application port
PORT=3000

# JWT signing secret
JWT_SECRET=your_jwt_secret_here

# Database (PostgreSQL)
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USERNAME=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DATABASE=tejada_hnos
